pipeline {
    agent any
    stages { 
        stage('Example') {
            steps {
                echo 'Hello World1'
            }
        }
        stage('Build the project'){
        steps{
        
        echo 'Build is ready'
        }
        }
        stage('Testing the build'){
        steps{

        echo 'Testing of the build is successfull'
        }
        }
        stage('Build deployment'){
        steps{
        
        echo 'Build is successfully deployed'
        }
        }
        stage('Build deployment in SIT environment'){
        steps{
        
        echo 'Build is working fine in SIT'
        }
        }


    }
   
}
