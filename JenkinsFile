pipeline {
    agent any
    stages { 
        stage('Example') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build the project'){
        steps{
        
        echo 'Build is ready and working fine'
        }
        }
        stage('Testing the provided build'){
        steps{

        echo 'Testing of the provided build is successfull'
        }
        }
        stage('Build deployment'){
        steps{
        
        echo 'Build is successfully deployed in production environment'
        }
        }
        stage('Post Deployment Activities'){
        steps{
        
        echo 'Application is working fine in prod environment'
        }
        }
    }
   
}
